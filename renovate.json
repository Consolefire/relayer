{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    ":dependencyDashboard",
    ":preserveSemverRange"
  ],
  "dependencyDashboard": true,
  "maven": {},
  "versioning": "semver",
  "rangeStrategy": "replace",
  "timezone": "Europe/Berlin",
  "major": {
    "enabled": true
  },
  "npm": {
    "enabled": false
  },
  "vulnerabilityAlerts": {
  },
  "packageRules": [
    {
      "matchManagers": [
        "maven"
      ],
      "registryUrls": [
        "https://repo1.maven.org/maven2/"
      ]
    }
  ]
}
